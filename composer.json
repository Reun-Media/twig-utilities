{
    "name": "reun/twig-utilities",
    "description": "Various Twig utilities and extensions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kimmo Salmela",
            "email": "cryszon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "twig/twig": "^3.0",
    },
    "autoload": {
        "psr-4": {"Reun\\TwigUtilities\\": "src/TwigUtilities"}
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "php-di/php-di": "^6.0"
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php tests/phpunit",
        "functional-test-server": "php -S localhost:8080 -t tests/functional/TwigUtilities"
    },
    "config": {
        "process-timeout": 0
    }
}
